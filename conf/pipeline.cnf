modules=java/jdk-13.33,python3-as-python
cmd_1=$[directories]pipeline/scripts/step_varscan.pl --fun call --mode SNP
varscan_bin=java -Djava.io.tmpdir=\$PBS_JOBFS -DGATK_STACKTRACE_ON_USER_EXCEPTION=true -Xms4000m -jar $[directories]tools/VarScan.v2.4.4.jar
bgzip_bin=$[directories]tools/htslib-1.10.2/bgzip
bcftools_bin=$[directories]tools/bcftools-1.10.2/bcftools
tabix_bin=$[directories]tools/htslib-1.10.2/tabix

min_coverage=8
min_reads2=5
min_avg_qual=15
min_var_freq=0.2
p_value=0.99

[step:split:strelka]
dir=strelka
pbs_queue=normal
pbs_mem=16GB
pbs_ncpus=8
pbs_walltime=00:30:00
pbs_jobfs=100GB
modules=python2-as-python
cmd_1=$[directories]pipeline/scripts/step_strelka.pl
run_bin=$[directories]tools/strelka-2.9.10/bin/configureStrelkaGermlineWorkflow.py
config_bin=$[directories]tools/strelka-2.9.10/bin/configureStrelkaGermlineWorkflow.py
bedintersect_bin=$[directories]tools/bedtools2/bin/bedtools intersect
bcftools_bin=$[directories]tools/bcftools-1.10.2/bcftools
bgzip_bin=$[directories]tools/htslib-1.10.2/bgzip
tabix_bin=$[directories]tools/htslib-1.10.2/tabix

[step:split:merge_callers]
dir=vcf
pbs_queue=normal
pbs_mem=4GB
pbs_ncpus=2
pbs_walltime=00:30:00
pbs_jobfs=100GB
cmd_1=$[directories]pipeline/scripts/step_mergecallers.pl
bcftools_bin=$[directories]tools/bcftools-1.10.2/bcftools
bgzip_bin=$[directories]tools/htslib-1.10.2/bgzip
tabix_bin=$[directories]tools/htslib-1.10.2/tabix
